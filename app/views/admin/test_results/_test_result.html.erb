<tr>
  <td>
    <%= test_result.id %>
  </td>
  <td>
    <%= link_to test_result.test_name, admin_test_result_path(test_result) %>
  </td>
  <td>
    <%= test_result.user.grade_and_class %>
  </td>
  <td>
    <%= test_result.user.decorate.full_name %>
  </td>
  <td>
    <%= l test_result.created_at, format: :long %>
  </td>
  <td>
    <div class="btn-group" role="group">
      <%= button_to t('defaults.show'), admin_test_result_path(test_result), class: 'btn btn-info me-2' %>
      <%= button_to t('defaults.edit'), edit_admin_test_result_path(test_result), class: 'btn btn-success me-2' %>
      <%= button_to t('defaults.delete'), admin_test_result_path(test_result), method: :delete, data: { turbo_confirm: "本当に削除しますか？" }, class: 'btn btn-danger' %>
    </div>  
  </td>
</tr>